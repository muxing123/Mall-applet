/* pages/specialdetail/specialdetail.wxss */
@import "../../lib/wxParse/wxParse.wxss";
.specialdetail {
  padding: 0 10rpx;
  background: #F2F3F4;
}
.specialdetail .wxParse-img{
  display: block;
}
.specialdetail .titleCenter {
  padding: 30rpx 0;
  text-align: center;
  color: #8a8a8a;
  font-size: 30rpx;
}
.specialdetail .item {
  background: white;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.specialdetail .item .pic {
  width: 100%;
  height: 380rpx;
}
.specialdetail .item .title {
  font-size: 26rpx;
  padding-top: 20rpx;
}